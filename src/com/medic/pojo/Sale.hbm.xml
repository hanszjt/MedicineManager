<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    
     <hibernate-mapping >
    	<class name="com.medic.pojo.Sale" table="sale">
    		<id name="id" column="id" type="integer">
    			<generator class="native"></generator>
    		</id>
    		<property name="sid" column="sid"  type="string"/>
    		
    		<property name="mid" column="mid"  type="integer"/>
    		<property name="mname" column="mname" type="string"/>
    		<property name="count" column="count" type="integer"/>
    		<property name="saleprice" column="saleprice" type="double"/>

    	</class>
    </hibernate-mapping>